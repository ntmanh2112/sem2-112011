package bai2;

public class Employee {
	public int id;
	public String name;
	public int age;
	public String company;
	public double salary;
	
	public Employee() {
	}
	
	public Employee(int id, String name, int age, String company, double salary) {
		this.id = id;
		this.name = name;
		this.age = age;
		this.company = company;
		this.salary = salary;
	}
	
	public void display() {
		
	}

}

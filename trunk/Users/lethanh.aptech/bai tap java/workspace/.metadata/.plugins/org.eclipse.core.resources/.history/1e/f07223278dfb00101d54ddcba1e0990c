package thread;

public class textThread {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		ThreadA threadA = new ThreadA();
		threadA.setName("Tien trinh 1");
		
		ThreadB b = new ThreadB();
		Thread threadB = new Thread(b);
		threadB.setName("Tien Trinh 2");
		
		threadA.start();
		threadB.start();
	}

}
